
@{
    ViewBag.Title = "Index";
}

    <center>
        <h2>Upload</h2>
        @using (Html.BeginForm("Index", "Files", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div>   
                @Html.TextBox("file", "", new { type = "file" });
                <input type="submit" value="Save Files" />

                @ViewBag.Message
            </div>
        }
    </center>





